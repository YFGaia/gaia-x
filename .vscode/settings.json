{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.tsc.autoDetect": "off",
  "json.schemas": [
    {
      "fileMatch": [
        "/*electron-builder.json5",
        "/*electron-builder.json"
      ],
      "url": "https://json.schemastore.org/electron-builder"
    }
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.git": true,
    "**/.github": true,
    "**/.DS_Store": true,
    "**/dist": true,
    "**/build": true,
    "**/coverage": true,
    "**/tmp": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/pnpm-lock.yaml": true,
    "**/*.min.*": true,
    "**/out": true,
    "**/.husky": true,
    "**/logs": true,
    "**/.next": true,
    "**/.nuxt": true,
    "**/*.log": true,
    "apps/client/release": true,
    "apps/client/dist-electron": true,
    "apps/client/dist": true,
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  }
}
